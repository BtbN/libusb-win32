TARGETNAME = testlibusb
TARGETPATH = obj
TARGETTYPE = PROGRAM
UMTYPE = console
USE_MSVCRT = 1
386_STDCALL = 0
USER_C_FLAGS = /Gd /O2
C_DEFINES = /DVERSION_MAJOR=0 /DVERSION_MINOR=1 /DVERSION_MICRO=10 \
            /DVERSION_NANO=2 /DRC_VERSION=0,1,10,2 /DVERSION="\"0.1.10.2\"" \
            /Dsnprintf=_snprintf



TARGETLIBS = $(SDK_LIB_PATH)\setupapi.lib \
             $(SDK_LIB_PATH)\kernel32.lib \
             $(SDK_LIB_PATH)\advapi32.lib \
             $(SDK_LIB_PATH)\user32.lib \
             $(SDK_LIB_PATH)\msvcrt.lib \
             libusb0.lib 

