TARGETNAME = testlibusb-win
TARGETPATH = output
TARGETTYPE = PROGRAM
UMTYPE = windows
UMENTRY = winmain
USE_MSVCRT = 1
386_STDCALL = 0
USER_C_FLAGS = /Gd /O2
C_DEFINES = /DVERSION_MAJOR=0 /DVERSION_MINOR=1 /DVERSION_MICRO=10 \
            /DVERSION_NANO=2 /INF_VERSION=0.1.10.2 \
            /DRC_VERSION=1,1,10,2 /DRC_VERSION_STR="\"1.1.10.2\"" \
            /Dvsnprintf=_vsnprintf



TARGETLIBS = $(SDK_LIB_PATH)\setupapi.lib \
             $(SDK_LIB_PATH)\kernel32.lib \
             $(SDK_LIB_PATH)\advapi32.lib \
             $(SDK_LIB_PATH)\user32.lib \
             libusb0.lib 

SOURCES = testlibusb_win.c testlibusb_win_rc.rc 
