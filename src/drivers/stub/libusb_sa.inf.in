[Version]
Signature = "$Chicago$"
Class     = USB
ClassGUID = {36FC9E60-C465-11CF-8056-444553540000}
provider  = %libusb-win32%
DriverVer = @INF_VERSION@

[SourceDisksNames]
1 = "Libusb-Win32 Stub Driver Installation Disk",,

[SourceDisksFiles]
libusbst.sys = 1,, 
libusbfl.sys = 1,, 
libusb0.dll = 1,, 

[Manufacturer]
%libusb-win32%=libusb-win32

[libusb-win32]

"LibUsb-Win32 Sample Device, Version @INF_VERSION@" = LIBUSB_STUB.Dev, USB\VID_0547&PID_2131

[DestinationDirs]
LIBUSB_STUB.Files.Sys = 10,System32\Drivers
LIBUSB_FILTER.Files.Dll = 10,System32

[LIBUSB_STUB.Files.Sys]
libusbst.sys
libusbfl.sys

[LIBUSB_STUB.Files.Dll]
libusb0.dll

[LIBUSB_STUB.Dev]
CopyFiles = LIBUSB_STUB.Files.Sys, LIBUSB_STUB.Files.Dll
AddReg    = LIBUSB_STUB.AddReg
DelReg    = LIBUSB_STUB.DelReg

[LIBUSB_STUB.Dev.NT]
CopyFiles = LIBUSB_STUB.Files.Sys, LIBUSB_STUB.Files.Dll
DelReg    = LIBUSB_STUB.DelReg.NT

[LIBUSB_STUB.Dev.HW]
AddReg = LIBUSB_STUB.AddReg.HW

[LIBUSB_STUB.Dev.NT.HW]
AddReg = LIBUSB_STUB.AddReg.NT.HW

[LIBUSB_STUB.Dev.NT.Services]
AddService = libusbst, 0x00000002, LIBUSB_STUB.AddService
AddService = libusbfl,, LIBUSB_FILTER.AddService

[LIBUSB_STUB.AddService]
DisplayName    = "LibUsb-Win32 - Stub Driver @INF_VERSION@"
ServiceType    = 1
StartType      = 3
ErrorControl   = 0
ServiceBinary  = %12%\libusbst.sys

[LIBUSB_FILTER.AddService]
DisplayName    = "LibUsb-Win32 - Filter Driver @INF_VERSION@"
ServiceType    = 1
StartType      = 3
ErrorControl   = 0
ServiceBinary  = %12%\libusbfl.sys

[LIBUSB_STUB.AddReg]
HKR,,DevLoader,,*ntkern
HKR,,NTMPDriver,,libusbst.sys

[LIBUSB_STUB.AddReg.HW]
HKR,,"LowerFilters",0x00010000,"libusbfl.sys";

[LIBUSB_STUB.AddReg.NT.HW]
HKR,,"LowerFilters",0x00010000,"libusbfl";

[LIBUSB_STUB.DelReg]
HKLM, Software\LibUsb-Win32, Version
HKLM, System\CurrentControlSet\Services\Class\USB, LowerFilters

[LIBUSB_STUB.DelReg.NT]
HKLM, Software\LibUsb-Win32, Version
HKLM, System\CurrentControlSet\Control\Class\{36FC9E60-C465-11CF-8056-444553540000}, LowerFilters

[Strings]
libusb-win32 = "http://libusb-win32.sourceforge.net"
