 FILEFLAGS 0x0L
 FILEOS 0x40004L
 FILETYPE 0x1L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "CompanyName", "http://libusb-win32.sourceforge.net"
	          VALUE "FileDescription", "LibUSB-Win32 - Kernel Driver"
            VALUE "FileVersion", "@VERSION@"
            VALUE "ProductVersion", "@VERSION@"
            VALUE "InternalName", "libusb-win32-driver"
            VALUE "LegalCopyright", "© 2002-2005 S. Meyer, <ste_meyer@web.de>"
            VALUE "OriginalFilename", "libusb0.sys"
            VALUE "ProductName", "LibUSB-Win32"
        END
    END
END
